<AnchorPane prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.History">
   <children>
      <Button fx:id="btnMatches" layoutX="287.0" layoutY="50.0" mnemonicParsing="false" onAction="#setWindMatches" prefHeight="25.0" prefWidth="101.0" text="Matches" />
      <Button fx:id="btnRating" layoutX="550.0" layoutY="50.0" mnemonicParsing="false" onAction="#setWindRating" prefHeight="25.0" prefWidth="101.0" text="Rating" />
      <Button fx:id="btnAccount" layoutX="152.0" layoutY="50.0" mnemonicParsing="false" onAction="#setWindAccount" prefHeight="25.0" prefWidth="101.0" text="Account" />
      <Label fx:id="nickname" alignment="CENTER" contentDisplay="CENTER" layoutX="41.0" layoutY="50.0" text="Nickname" />
      <Label fx:id="balance" alignment="CENTER" contentDisplay="CENTER" layoutX="41.0" layoutY="101.0" text="Dota point" />
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="41.0" layoutY="77.0" text="Balance" />
      <Button fx:id="btn_getPoint" layoutX="34.0" layoutY="126.0" mnemonicParsing="false" onAction="#getPoint" prefHeight="38.0" prefWidth="72.0" text="Get Points" />
      <Label layoutX="437.0" layoutY="49.0" text="History">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="tableHistory" layoutX="138.0" layoutY="91.0" prefHeight="650.0" prefWidth="850.0">
        <columns>
          <TableColumn fx:id="colID" prefWidth="39.0" text="№" />
          <TableColumn fx:id="colTime" prefWidth="147.0" text="Time" />
            <TableColumn fx:id="colMatchID" prefWidth="63.0" text="Match" />
            <TableColumn fx:id="colDescript" prefWidth="288.0" text="Description" />
            <TableColumn fx:id="colBalance" prefWidth="104.0" text="Balance" />
            <TableColumn fx:id="colBet" prefWidth="95.0" text="Bet" />
            <TableColumn fx:id="colChange" prefWidth="94.0" text="Сhange" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
